{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "./dist",
		"inlineSourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"skipLibCheck": true,
		"composite": true,
		"tsBuildInfoFile": "dist/.tsbuildinfo.json"
	},
	"include": ["src"],
	"exclude": ["**/*.test.ts"]
}
